namespace clients.c40;

import datacenter;

VM[1..6]: c60m300<clone,boot=4, shutdown=5>;
VM[7..15]: c50m300<clone,boot=9, shutdown=10>;
VM[16..18]: c50m100<clone,boot=5, shutdown=6>;

$T1 = VM[1..6];
$T2 = VM[7..15];
$T3 = VM[16..18];
for $t in $T[1..3] {
	spread($t);
}
among($T3, $datacenter.leafs);
export $me to *;