namespace clients.c270;

import datacenter;

VM[1..2]: c50m200<clone,boot=4, shutdown=5>;
VM[3..7]: c60m100<clone,boot=9, shutdown=10>;
VM[8..10]: c30m200<clone,boot=5, shutdown=6>;

$T1 = VM[1..2];
$T2 = VM[3..7];
$T3 = VM[8..10];
for $t in $T[1..3] {
	spread($t);
}
among($T3, $datacenter.leafs);
export $me to *;