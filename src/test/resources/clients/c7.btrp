namespace clients.c7;

import datacenter;

VM[1..6]: c40m300<clone,boot=4, shutdown=5>;
VM[7..9]: c50m100<clone,boot=9, shutdown=10>;
VM[10..19]: c40m200<clone,boot=5, shutdown=6>;

$T1 = VM[1..6];
$T2 = VM[7..9];
$T3 = VM[10..19];
for $t in $T[1..3] {
	spread($t);
}
among($T3, $datacenter.leafs);
export $me to *;